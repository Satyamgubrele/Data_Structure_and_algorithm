#include<stdio.h>
#include<conio.h>
void main(){
    int a[100],item,i,n;
    printf("enter the size of an array");
    scanf("%d",&n);
    printf("enter the item");
    scanf("%d",&item);
    
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);

    }
    for (i=0;i<n;i++)
    {
        if(a[i]==item){
            printf("item found on location %d",i);
            break;

        }
        else if(i==n){
            printf("item not found");
        }
    }


}
